<%- include('partials/header'); -%>
<h1>Home</h1>
<p> <%= homeContent %> </p>


<%posts.forEach(function(post) {%>
<h2><%= post.title; %></h2>
<% const fullPostLength = post.content; %>
<p><%= fullPostLength.substring(0, 100) + "..."%> <a href="<%= "/posts/" + post.title %>">Read More</a> </p>
<%});%>

<%- include('partials/footer'); -%>
